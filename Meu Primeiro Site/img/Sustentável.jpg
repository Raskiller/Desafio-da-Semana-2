            var urlDigitada = output.substring(0, output.length - 2);

            console.log(urlDigitada);

            if (urlDigitada) {
                console.log('submit');
                var textarea = jQuery(this).find("textarea").val();
                textarea = textarea.replace(urlDigitada, '');
                jQuery(this).find("textarea").val(textarea);
                console.log('digitou url');

                jQuery('<span class="error" style="width: 220px;color: #F44336;text-transform: lowercase;padding: 7px;font-size: 11px;margin-top: -13px;position:absolute;top:78%;left:0;margin-top:0;z-index:5;">Informação inválida! Não use links</span>').insertAfter(jQuery(this).find("textarea"));

                return false;
            } else {
                return true;
            }
        });

    });

</script>

<script>
    jQuery(document).ready(function() {
        var email_ok = "";
        var tel_ok = "";

        jQuery('input[type="email"]').each(function() {
            jQuery(this).blur(function() {
                email = jQuery(this).val();
                bt = jQuery(this).closest('form').find('button');
                bt_input = jQuery(this).closest('form').find('#enviar');
                jQuery.get('https://www.ibccoaching.com.br/check-email/?url-test=' + email,
                    function(data, status) {
                        console.log(data);
                        if (data == 1) {
                            jQuery('input[type="email"]').css({
                                'border': '1px solid green',
                                'background-color': 'green',
                                'color': 'white important',
                                '-webkit-box-shadow': '0 0 0 20px green inset'
                            });
                            email_ok = 1;
                            if (email_ok == 1 || tel_ok == 1) {
                                console.log(email_ok);
                                jQuery(bt).removeAttr('disabled');
                                jQuery(bt_input).removeAttr('disabled');
                            }
                        } else {
                            function faca_email() {
                                jQuery('input[type="email"]').val("E-mail incorreto");
