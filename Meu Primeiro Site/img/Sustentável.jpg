                if (!filter.test(tel)) {
                    function faca_tel() {
                        $('input[type="tel"]').val("Telefone incorreto");
                    }

                    function limpa_tel() {
                        jQuery('input[type="tel"]').val("");
                    }
                    faca_tel();
                    jQuery('input[type="tel"]').css({
                        'border': '1px solid #F44336',
                        'background': '#F44336',
                        '-webkit-box-shadow': '0 0 0 20px #F44336 inset',
                    });
                    jQuery(this).click(function() {
                        limpa_tel();
                    });
                    jQuery(bt).attr('disabled', '');
                    jQuery(bt_input).attr('disabled', '');
                } else {
                    console.log('ok');
                    jQuery('input[type="tel"]').css({
                        'border': '1px solid green',
                        'background-color': 'green',
                        '-webkit-box-shadow': '0 0 0 20px green inset'
                    });
                    tel_ok = 1;
                    if (email_ok == 1 && tel_ok == 1) {
                        console.log(tel_ok);
                        jQuery(bt).removeAttr('disabled');
                        jQuery(bt_input).removeAttr('disabled');
                    }
                }
            });
        });
    });

</script>
<script>
    var maskBehavior = function(val) {
            return val.length === 15 ? '(00) 00000-0000' : '(00) 0000-00009';
        },
        options = {
            onKeyPress: function(val, e, field, options) {
                field.mask(maskBehavior.apply({}, arguments), options);
                if (field[0].value.length >= 14) {
                    var val = field[0].value.replace(/\D/g, '');
                    if (/\d\d(\d)\1{7,9}/.test(val)) {
                        field[0].value = '';
                        jQuery('input[type="tel"]').val('Telefone incorreto')
                        jQuery('input[type="tel"]').css({
                            'border': '1px solid #F44336',
                            'background': '#F44336',
                            '-webkit-box-shadow': '0 0 0 20px #F44336 inset',
                        });
                        jQuery(this).click(function() {
                            jQuery('input[type="tel"]').val('')
                        });
                    }
                }
            }
        };
    jQuery("input[type='tel']").mask(maskBehavior, options);

</script>


